{
  "jsc": {
    "target": "es2022",
    "parser": {
      "syntax": "typescript"
    },
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "module": {
    "type": "es6",
    "resolveFully": true
  },
  "exclude": "^(.*/__tests__/.*|.*\\.test.ts|.*\\.spec.ts)$",
  "sourceMaps": true
}
